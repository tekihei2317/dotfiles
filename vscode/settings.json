{
  "keyboard.dispatch": "keyCode",
  "editor.fontSize": 14,
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.lineHeight": 14,
  "editor.foldingStrategy": "indentation",
  "editor.foldingHighlight": false,
  "emmet.triggerExpansionOnTab": true,
  "emmet.excludeLanguages": ["markdown"],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "auto-close-tag.activationOnLanguage": [
    "php",
    "blade",
    "plaintext",
    "markdown",
    "vue",
    "html"
  ],
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[graphql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[ruby]": {
    // "editor.defaultFormatter": "jnbt.vscode-rufo"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "ruby.format": "rufo",
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[html]": {
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": ["document"],
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[blade]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "shufo.vscode-blade-formatter",
    "editor.autoClosingBrackets": "always"
  },
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "[sql]": {
    "editor.formatOnSave": false
  },
  "[svg]": {
    "editor.formatOnSave": false
  },
  "[php]": {
    "editor.tabSize": 4
  },
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": "HackGen, 'Ricty Diminished', Menlo, Monaco, monospace, Ubuntu",
  "rufo.exe": "/Users/gib0017/.anyenv/envs/rbenv/shims/rufo",
  "editor.rulers": [80, 120],
  "vetur.grammar.customBlocks": {
    "docs": "md",
    "i18n": "json",
    "page-query": "graphql",
    "static-query": "graphql"
  },
  "hediet.vscode-drawio.local-storage": "",
  "intelephense.diagnostics.undefinedClassConstants": false,
  "intelephense.diagnostics.undefinedConstants": false,
  "intelephense.diagnostics.undefinedFunctions": false,
  "intelephense.diagnostics.undefinedMethods": false,
  "intelephense.diagnostics.undefinedProperties": false,
  "intelephense.diagnostics.undefinedSymbols": false,
  "intelephense.diagnostics.undefinedTypes": false,
  "intelephense.diagnostics.undefinedVariables": false,
  "intelephense.diagnostics.unusedSymbols": false,
  "intelephense.environment.phpVersion": "8.0.10",
  "php.validate.executablePath": "",
  // "terminal.integrated.gpuAcceleration": "no",
  "workbench.startupEditor": "none",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorTheme": "GitHub Dark",
  "clang-format.executable": "/usr/local/bin/clang-format",
  "zenMode.hideTabs": false,
  "zenMode.centerLayout": false,
  "editor.minimap.enabled": false,
  "workbench.panel.defaultLocation": "right",
  "editor.acceptSuggestionOnEnter": "off",
  "workbench.editor.enablePreview": false,
  "ruby.lint": {
    // "rubocop": true
  },
  "blade.format.enable": true,
  "bladeFormatter.format.indentSize": 2,
  "workbench.editor.focusRecentEditorAfterClose": false,
  "markdownlint.run": "onSave",
  "terminal.integrated.defaultProfile.linux": "tmux",
  "files.trimFinalNewlines": true,
  "editor.formatOnSave": true,
  "markdown-preview-enhanced.enableExtendedTableSyntax": true,
  "terminal.integrated.fontSize": 14,

  // vim
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<Enter>"],
      "commands": ["editor.action.insertLineAfter"]
    }
  ],
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "/Users/gib0017/.vimrc",
  "vim.useSystemClipboard": true,
  "vim.easymotion": true,
  "vim.easymotionKeys": "sdfghjkl",
  "vim.hlsearch": true,
  "markdown-preview-enhanced.previewTheme": "github-light.css",
  "liveServer.settings.donotShowInfoMsg": true,
  "files.exclude": {
    "/.git": true,
    "/.DS_Store": true,
    "/node_modules": true,
    "/node_modules/": true
  },
  "files.watcherExclude": {
    "/node_modules/": true
  },
  "explorer.compactFolders": false,
  "editor.accessibilitySupport": "off",
  "terminal.integrated.defaultLocation": "editor",
  "tailwindCSS.includeLanguages": {
    "blade": "html"
  },
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "redhat.telemetry.enabled": true,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "remote.SSH.defaultExtensions": ["gitpod.gitpod-remote-ssh"],
  "remote.SSH.configFile": "~/.ssh/config",
  "workbench.activityBar.visible": false
}
