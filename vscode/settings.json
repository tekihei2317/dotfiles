{
  "keyboard.dispatch": "keyCode",
  "editor.fontSize": 14,
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.lineHeight": 14,
  "editor.foldingStrategy": "indentation",
  "editor.foldingHighlight": false,
  "emmet.triggerExpansionOnTab": true,
  "emmet.excludeLanguages": ["markdown"],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[graphql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": ["document"],
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[blade]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "shufo.vscode-blade-formatter",
    "editor.autoClosingBrackets": "always"
  },
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "[sql]": {
    "editor.formatOnSave": false
  },
  "[svg]": {
    "editor.formatOnSave": false
  },
  "[php]": {
    "editor.tabSize": 4
  },
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": "HackGen, 'Ricty Diminished', Menlo, Monaco, monospace, Ubuntu",
  "editor.rulers": [80, 120],
  "intelephense.diagnostics.undefinedClassConstants": false,
  "intelephense.diagnostics.undefinedConstants": false,
  "intelephense.diagnostics.undefinedFunctions": false,
  "intelephense.diagnostics.undefinedMethods": false,
  "intelephense.diagnostics.undefinedProperties": false,
  "intelephense.diagnostics.undefinedSymbols": false,
  "intelephense.diagnostics.undefinedTypes": false,
  "intelephense.diagnostics.undefinedVariables": false,
  "intelephense.environment.phpVersion": "8.1.3",
  "php.validate.executablePath": "",
  "workbench.startupEditor": "none",
  "editor.suggestSelection": "first",
  "workbench.colorTheme": "GitHub Dark",
  "zenMode.hideTabs": false,
  "zenMode.centerLayout": false,
  "editor.minimap.enabled": false,
  "editor.acceptSuggestionOnEnter": "off",
  "workbench.editor.enablePreview": false,
  "blade.format.enable": true,
  "bladeFormatter.format.indentSize": 2,
  "workbench.editor.focusRecentEditorAfterClose": false,
  "terminal.integrated.defaultProfile.linux": "tmux",
  "files.trimFinalNewlines": true,
  "markdown-preview-enhanced.enableExtendedTableSyntax": true,
  "terminal.integrated.fontSize": 14,

  // vim
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<Enter>"],
      "commands": ["editor.action.insertLineAfter"]
    }
  ],
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "/Users/gib0017/.vimrc",
  "vim.useSystemClipboard": true,
  "vim.easymotion": true,
  "vim.easymotionKeys": "sdfghjkl",
  "vim.hlsearch": true,
  "markdown-preview-enhanced.previewTheme": "github-light.css",
  "files.exclude": {
    "/.git": true,
    "/.DS_Store": true,
    "/node_modules": true,
    "/node_modules/": true
  },
  "files.watcherExclude": {
    "/node_modules/": true
  },
  "explorer.compactFolders": false,
  "editor.accessibilitySupport": "off",
  "tailwindCSS.includeLanguages": {
    "blade": "html"
  },
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "redhat.telemetry.enabled": true,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.formatOnSave": true,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "database-client.disableSqlCodeLen": true,
  "remote.SSH.configFile": "/Users/gib0017/.ssh/config",
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.unicodeHighlight.nonBasicASCII": false,
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "git.autofetch": true,
  "githubPullRequests.pullBranch": "never"
}
